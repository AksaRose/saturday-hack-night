{
    "name": "saturday-hacknight",
    "version": "1.0.0",
    "description": "Saturday HackNight Platform",
    "license": "MIT",
    "scripts": {
        "build": "turbo run build",
        "build:web": "turbo run build --filter=web",
        "build:web-legacy": "turbo run build --filter=web-legacy",
        "start": "turbo run start",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "turbo run format"
    },
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "latest",
        "turbo": "latest"
    }
}
